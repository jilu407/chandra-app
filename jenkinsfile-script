node {
    stage('checkout scm ')
    {
        git url: 'https://github.com/jilu407/chandra-app.git'
    }
    stage('install')
    {
        sh 'npm install'
    }
    stage('build')
    {
        sh 'npm run build --prod'

    }
    stage('Deploy'){
            
        sh 'cp -r /var/lib/jenkins/workspace/script-pipeline-angular/dist/chandra-app/* /usr/share/nginx/html/'
      }
}
